amount = 4000
local filename = game:GetService("Players").LocalPlayer.Name .. ".txt"
token = game:GetService("Players").LocalPlayer.PlayerGui.MainGUI.Lobby.Screens.Shop.Title.Coins.Container.Amount.Text
print(token)
if tonumber(token) >= amount then
    print("True")
    writefile(filename, "Yummytool")
else print("False")
end
