amount = 8000
local filename = game:GetService("Players").LocalPlayer.Name .. ".txt"
token = game:GetService("Players").LocalPlayer.PlayerGui.CrossPlatform.Christmas2024.Container.EventFrames.MysteryBox.Info.Tokens.Container.TextLabel.Text
print(token)
if tonumber(token) >= amount then
    print("True")
    writefile(filename, "Yummytool")
else print("False")
end
